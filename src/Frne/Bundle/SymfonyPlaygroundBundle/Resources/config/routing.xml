<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="home" path="/">
        <default key="_controller">FrneSymfonyPlaygroundBundle:Default:index</default>
    </route>
    <route id="search_index" path="/search">
        <default key="_controller">FrneSymfonyPlaygroundBundle:FulltextSearch:index</default>
    </route>
    <route id="search_hydration" path="/search/hydrate">
        <default key="_controller">FrneSymfonyPlaygroundBundle:HydratingSearch:index</default>
    </route>
</routes>
